<form action="/showlist" method="get">
    <select name="list" id="list">
        <%listData.forEach(list=>{%>
        <option value="<%=list.list%>"><%=list.list%></option>
        <%})%>
    </select>
    <button type="submit">Select</button>
</form>

<table class="tableBlue">
    <thead>
        <tr>
            <th>ID</th>
            <th>Word</th>
            <th>Definition</th>
            <th>Example</th>
            <th>synonyms</th>
            <th>Image</th>
            <th>audio</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>
        <% allList.forEach(val =>{ %>
            <tr>
                <td><%=val.id%></td>
                <td><%=val.word%></td>
                <td><%=val.definition%></td>
                <td><%=val.example%></td>
                <td><%=val.synonyms%></td>
                <td><%=val.img_url%></td>
                <td><%=val.audio%></td>
                <td><a href="/deletUpdateForm/<%=val.id%>"><p>&#x270F</p></a></td>
            </tr>
      <% }) %>
    </tbody>
</table>